{
	"name": "lawtonwebdept/kh-news",
	"description": "News module for SilverStripe 6",
	"type": "silverstripe-vendormodule",
	"keywords": ["lawtonwebdept", "news"],
	"authors": [
		{"name": "Lawton Printing"}
	],
	"require": {
		"silverstripe/cms": "^6",
		"silverstripe/framework": "^6"
	},
	"autoload": {
		"psr-4": {
			"lawtonwebdept\\kh-news\\": "src/"
		}
	},
	"extra": {
		"installer-name": "kh-news",
		"expose": [
			"client/images"
		]
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"silverstripe/vendor-plugin": true
		}
	}
}
